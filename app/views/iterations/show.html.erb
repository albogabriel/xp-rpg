<p id="notice"><%= notice %></p>

<p>
  <b>Id:</b>
  <%= @iteration.id %>
</p>

<p>
  <b>Label:</b>
  <%= @iteration.label %>
</p>

<h2>Stories</h2>
<% @iteration.stories.each do |story| %>
  <p>
    <b>Title:</b>
    <%= story.title %>
  </p>

<% end %>


<%= link_to 'Edit', edit_iteration_path(@iteration) %> |
<%= link_to 'Back', iterations_path %>
